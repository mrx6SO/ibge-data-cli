{
  "name": "@rmderojr/ibge-data-cli",
  "version": "1.2.0",
  "description": "A CLI tool to fetch, store, and query Brazilian location data from the IBGE API.",
  "main": "dist/app.js",
  "bin": {
    "ibge-data-cli": "./dist/app.js"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE.md"
  ],
  "scripts": {
    "start": "node dist/app.js",
    "build": "rm -rf dist && tsc",
    "test": "jest",
    "knex:migrate": "knex migrate:latest --knexfile src/knexfile.ts"
  },
  "keywords": [
    "ibge",
    "api",
    "cli",
    "data",
    "brazil",
    "brazil-data"
  ],
  "author": "Roberto Mardero Junior",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "commander": "^12.0.0",
    "csv-writer": "^1.6.0",
    "knex": "^3.0.1",
    "sqlite3": "^5.1.6",
    "tslib": "^2.6.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  }
}
